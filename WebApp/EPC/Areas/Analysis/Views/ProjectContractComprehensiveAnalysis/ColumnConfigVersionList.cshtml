@{
    ViewBag.Title = "ColumnConfigVersionList";
}
<div id="Name" class="mini-layout" style="width: 100%; height: 100%;" splitsize="3">
    <div title="版本信息" region="west" width="250" expanded="true" showspliticon="false">
        <div class="mini-toolbar" style="padding: 7px; border-top: 0; border-left: 0; border-right: 0;">
            <table>
                <tr>
                    <td style="width: 100%;">
                        <a class="mini-button"
                           iconcls="icon-add" onclick="addVersion();">增加版本</a>
                        <a class='mini-button' id='btnSelect' iconcls='icon-save' text='确认' enabled='true' onclick='returnList();'></a>
                    </td>
                    <td style="white-space: nowrap;"></td>
                </tr>
            </table>
        </div>
        <div class="mini-fit">
            <div id="columnDataGrid" class="mini-datagrid" showpager="false" style="width: 100%;
                        height: 100%;" url="" multiselect="true" borderstyle="border-left: 0; border-right: 0;">
                <div property="columns">
                    <div type="checkcolumn">
                    </div>
                    <div field="Name" width="200" allowsort="true" align="center">
                        版本名称
                    </div>
                    <div field="Name" width="200" allowsort="true" align="center">
                        创建日期
                    </div>
                    <div field="IsLink" width="200" allowsort="true" align="center">
                        创建人
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div title="center" region="center" style="border-top: 0; border-right: 0; border-bottom: 0;">
        <div class="mini-fit">
            <div id="columnDataGrid" class="mini-datagrid" showpager="false" style="width: 100%;
                        height: 100%;" url="" multiselect="true" borderstyle="border-left: 0; border-right: 0;">
                <div property="columns">
                    <div type="checkcolumn">
                    </div>
                    <div field="Name" width="200" allowsort="true" align="center">
                        一级列名
                    </div>
                    <div field="Name" width="200" allowsort="true" align="center">
                        二级列名
                    </div>
                    <div field="IsLink" width="200" allowsort="true" align="center">
                        是否超链接
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<script type="text/javascript">

    function addVersion() {
        var url = "/EPC/Analysis/ProjectContractComprehensiveAnalysis/ColumnConfig";
        openWindow(url, {
            title: "选择查询版本"
        });
    }
</script>
