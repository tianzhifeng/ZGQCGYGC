@{
    ViewBag.Title = "增加文档";
}

<script src="~/Scripts/Main/js/partial.js" type="text/javascript"></script>

<form id="dataForm" method="post" align="center" autogetdata="true">
    <input name="EngineeringInfoID" class="mini-hidden" />
    <input name="ParticipationType" class="mini-hidden" />
    <input name="RelateObjType" class="mini-hidden" />
    <input name="RelateObjID" class="mini-hidden" />
    <input name="ID" class="mini-hidden" />
    <div class="formDiv" style="background-color: #fff;padding-top:55px">
        <script type='text/javascript'>
        </script><h1 align="center">
            文件信息管理
        </h1>
        <fieldset class="formDiv">
            <legend>基本信息</legend>
            <table class="ke-zeroborder" style="width:100%;" cellpadding="2" border="0">
                <tbody>
                    <tr>
                        <td style="width:15%;"></td>
                        <td style="width:35%;"></td>
                        <td style="padding-left:20px;"></td>
                        <td style="width:35%;"></td>
                    </tr>
                    <tr>
                        <td>
                            文件名称
                        </td>
                        <td colspan="3">
                            <input name='Name' class='mini-textbox' required='true' allowinput='true' style='width:100%;' maxlength='200' />
                        </td>
                    </tr>
                    <tr>
                        <td>
                            文件编号
                        </td>
                        <td>
                            <input name='Code' class='mini-textbox' required='true' style='width:100%' maxlength='200' />
                        </td>
                        <td style="padding-left:20px;">
                            文件分类
                        </td>
                        <td>
                            <input name='FileCategory' class='mini-combobox' required='false' allowinput='false' multiselect='false' shownullitem='true' valuefromselect='true' style='width:100%;' data='CooperationFileCategory' />
                        </td>
                    </tr>
                    <tr>
                        <td>
                            主文件
                        </td>
                        <td colspan="3">
                            <input name='FileID' class='mini-singlefile' style='width:100%' />
                        </td>
                    </tr>
                    <tr>
                        <td>
                            关键字
                        </td>
                        <td colspan="3">
                            <input name='KeyWords' class='mini-textbox' style='width:100%' maxlength='200' />
                        </td>
                    </tr>
                    <tr>
                        <td>
                            编制人
                        </td>
                        <td>
                            <input name='CreateUserID' textname='CreateUserName' class='mini-buttonedit' allowinput='true' style='width:100%;' enabled='false' />
                        </td>
                        <td style="padding-left:20px;">
                            编制单位
                        </td>
                        <td>
                            <input name='Participation' textname='ParticipationName' class='mini-buttonedit' allowinput='true' style='width:100%;' enabled='false' />
                        </td>
                    </tr>
                    <tr>
                        <td>
                            备注
                        </td>
                        <td colspan="3">
                            <input name='Remark' class='mini-textarea' required='false' allowinput='true' style='width:100%;height:100px;' maxlength='500' />
                        </td>
                    </tr>
                </tbody>
            </table>
        </fieldset>

    </div>
</form>
<div class="mini-toolbar" id="btnDiv" style="width: 100%; position: fixed; top: 0;
        left: 0; z-index: 100;">
    <table>
        <tr>
            <td>
                <a id="btnSave" class="mini-button" plain="true" iconcls="icon-save" onclick="save();">
                    保存
                </a> <a id="btnCancel" class="mini-button" plain="true" iconcls="icon-cancel" onclick="closeWindow()">
                    取消
                </a>
            </td>
            <td id="btnRight"></td>
        </tr>
    </table>
</div>
<script>
    @Html.GetEnum("EPC.CooperationFileCategory")
    //var enum_S_D_Document_FileCategory = [{ "value": "公文", "text": "公文" }, { "value": "批文", "text": "批文" }, { "value": "联系单", "text": "联系单" }, { "value": "其他", "text": "其他" }];
</script>
