@{
    ViewBag.Title = "Edit";
}

<div class="mini-toolbar" id="btnDiv" style="width:100%;position: fixed">
    <table>
        <tr>
            <td>
                <a class="mini-button" plain="true" iconcls="icon-save" onclick="save();">保存</a>
                <a class="mini-button" plain="true" iconcls="icon-cancel" onclick="closeWindow()">取消</a>
            </td>
        </tr>
    </table>
</div>

<form id="dataForm" method="post" style="top:30px;position:relative">
    <input name="ID" class="mini-hidden" />
    <input name="EngineeringInfo" class="mini-hidden" />
    <input name="MainCompany" class="mini-hidden" />
    <input name="MainCompanyName" class="mini-hidden" />


    <div class="formDiv">
        <h1 align="center">工作联系单</h1>

        <fieldset class="formDiv">
            <legend>基本信息</legend>
            <table class="ke-zeroborder" style="width:100%;" cellpadding="2" border="0">
                <tbody>
                    <tr>
                        <td style="width:15%;">
                            <br />
                        </td>
                        <td style="width:35%;">
                            <br />
                        </td>
                        <td style="width:15%;">
                            <br />
                        </td>
                        <td>
                            <br />
                        </td>
                    </tr>
                    <tr>
                        <td>名称</td>
                        <td>
                            <input name='Name' class='mini-textbox' required='true' style='width:100%;' maxLength='200' />
                        </td>
                        <td style="padding-left:20px;">
                            联系单编号
                        </td>
                        <td>
                            <input name='SerialNumber' class='mini-textbox' required='true' enabled="false" style='width:100%;' maxLength='200' />
                        </td>
                    </tr>
                    <tr>
                        <td>
                            项目名称
                        </td>
                        <td>
                            <input name="EngineeringInfo" class="mini-hidden" />
                            <input name='EngineeringInfoName' class='mini-textbox' required='true' enabled="false" style='width:100%;' />
                        </td>
                        <td style="padding-left:20px;">
                            项目编号
                        </td>
                        <td>
                            <input name='Code' class='mini-textbox' required='true' enabled="false" style='width:100%;' maxLength='200' />
                        </td>
                    </tr>
                    <tr>
                        <td>
                            发送单位
                        </td>
                        <td colspan="3">
                            <input name="SendCompany" class="mini-hidden" />
                            <input name='SendCompanyName' class='mini-textbox' required='true' enabled="false" style='width:100%;' maxLength='200' /><br />
                        </td>

                    </tr>

                </tbody>
            </table>
        </fieldset>

        <fieldset class="formDiv">
            <legend>其他</legend>
            <table class="ke-zeroborder" style="width:100%;" cellpadding="2" border="0">
                <tbody>
                    <tr>
                        <td style="width:15%;">
                            <br />
                        </td>
                        <td style="width:35%;">
                            <br />
                        </td>
                        <td style="width:15%;">
                            <br />
                        </td>
                        <td>
                            <br />
                        </td>
                    </tr>
                    <tr>
                        <td style="width:15%;">
                            事由
                        </td>
                        <td style="width:35%;" colspan="3">
                            <input name='Cause' class='mini-textarea' required='true' style='width:100%;height:50px;' maxLength='500' />
                        </td>
                    </tr>
                    <tr>
                        <td style="width:15%;">
                            内容
                        </td>
                        <td style="width:35%;" colspan="3">
                            <input name='Content' class='mini-textarea' style='width:100%;height:50px;' maxLength='500' />
                        </td>
                    </tr>
                </tbody>
            </table>
        </fieldset>

    </div>

</form>

<script type="text/javascript">


</script>

