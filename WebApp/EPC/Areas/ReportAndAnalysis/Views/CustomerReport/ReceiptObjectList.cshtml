@{
    ViewBag.Title = "ReceiptObjectList";
}
<div class="mini-toolbar" style="padding: 2px; border-bottom: 0;">
    <table style="width: 100%;">
        <tr>
            <td style="width: 100%;"></td>
            <td style="white-space: nowrap;">
                <input class="mini-buttonedit searchbox" id="key" emptytext="请输入收款项名称或合同名称或合同编号" style="width: 300px;"
                       onenter="quickSearch('Name,ContractName,ContractCode');" onbuttonclick="quickSearch('Name,ContractName,ContractCode');" />
            </td>
        </tr>
    </table>
</div>
<div class="mini-fit" style="height: 100px;">
    <div id="dataGrid" url="GetReceiptObjectList" class="mini-datagrid" style="width: 100%; height: 100%;"
         idfield="ID" multiselect="false" showpager="false" ondrawsummarycell="onDrawSummaryCell" showsummaryrow="true"
         sortfield="ContractName,PlanFinishDate" sortorder="asc,asc">
        <div property="columns">
            <div type="indexcolumn">
            </div>
            <div field="ContractCode" width="150" headeralign="center" allowsort="true">
                合同编号
            </div>
            <div field="ContractName" width="300" headeralign="center" allowsort="true">
                合同名称
            </div>
            <div field="Name" headeralign="center" width="180" align="left" allowsort="true">
                收款项名称
            </div>
            @*<div field="ChargeUserName" width="80" headeralign="center" align="center" allowsort="true">
                负责人
            </div>*@
            <div field="ReceiptValue" summarytype="sum" datatype="currency" currencyunit="￥" headeralign="center"
                 width="100" align="right" allowsort="true">
                收款项金额（元）
            </div>
            <div field="FactInvoiceValue" summarytype="sum" datatype="currency" currencyunit="￥" headeralign="center"
                 width="100" align="right" allowsort="true">
                已开票（元）
            </div>
            <div field="FactReceiptValue" summarytype="sum" datatype="currency" currencyunit="￥" headeralign="center"
                 width="100" align="right" allowsort="true">
                已到款（元）
            </div>
            <div field="BadDebtValue" summarytype="sum" datatype="currency" currencyunit="￥" headeralign="center"
                 width="100" align="right" allowsort="true">
                坏账（元）
            </div>
            <div field="OriginalPlanFinishDate" dateformat="yyyy-MM" headeralign="center" width="100" align="right"
                 allowsort="true">
                原计划年月
            </div>
            <div field="PlanFinishDate" dateformat="yyyy-MM" headeralign="center" width="100" align="center" allowsort="true">
                计划年月
            </div>
            <div field="ProjectInfoName" headeralign="center" width="260" align="left" allowsort="true">
                关联项目
            </div>
            <div field="Remark" headeralign="center" align="left" width="200" allowsort="true">
                备注
            </div>
        </div>
    </div>
</div>
<script type="text/javascript">
</script>
<script type="text/javascript">
    function onDrawSummaryCell(e) {
        e.cellStyle = " text-align: right;color:red";
    }
</script>
