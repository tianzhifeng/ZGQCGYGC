@{
    ViewBag.Title = "培训覆盖率统计";
}
<div class="mini-toolbar" style="border-bottom: 0; padding: 0px;">
    <table style="width: 100%;">
        <tr>
            <td style="width: 100%;">
            </td>
            <td style="white-space: nowrap;">
                <input id='key' class='mini-buttonedit gw-searchbox' emptytext='请输入承办部门' onenter="quickSearch('DeptName');"
                    onbuttonclick="quickSearch('DeptName');" />
                <a class="mini-button" onclick="quickSearch('DeptName');">查询</a>
            </td>
        </tr>
    </table>
</div>
<div class='mini-fit' id='divGrid'>
    <div id="dataGrid" class="mini-datagrid" style="width: 100%; height: 100%;" allowresize="true"
        url="GetTrainCoverageAreaList" idfield="DeptID" multiselect="true">
        <div property="columns">
            <div type="indexcolumn">
            </div>
            <div type="checkcolumn">
            </div>
            <div field="Trainyear" width="80" headeralign="center" allowsort="true" align="center">
                培训年度</div>
            <div field="DeptName" width="120" headeralign="center" allowsort="true" align="center">
                承办部门</div>
            <div field="UserCount" width="120" headeralign="center" allowsort="true" align="center">
                部门总人数</div>
            <div field="Hours" width="120" headeralign="center" align='right' allowsort="true">
                累计参训总学时</div>
            <div field="HoursScale" width="120" headeralign="center" align='right' allowsort="true">
                总体培训覆盖率(%)</div>
            <div header="公司级培训" headeralign="center">
                <div property="columns">
                    <div field="totalHours" width="80" align='right'>
                        参训总学时</div>
                    <div field="UserScale" width="80" align='right'>
                        培训覆盖率</div>
                </div>
            </div>
            <div header="部门级培训" headeralign="center">
                <div property="columns">
                    <div field="totalHours_Dept" width="80" align='right'>
                        参训总学时</div>
                    <div field="UserScale_Dept" width="80" align='right'>
                        培训覆盖率</div>
                </div>
            </div>
        </div>
    </div>
</div>
<script type="text/javascript">
    
    
</script>
