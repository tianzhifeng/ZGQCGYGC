@section scripts{
    <script src="@Url.Content("/MvcConfig/Scripts/layout.js")" type="text/javascript"></script>
    <script src="@Url.Content("/MvcConfig/Scripts/form.js")" type="text/javascript"></script>
}

<div style="margin-bottom: 5px; height: 22px; text-align: right; padding-right: 20px; background-color: rgb(247, 247, 247);">
    <a class="mini-button" iconcls="icon-save" onclick="saveForm()" id="btnSave">保存</a>
</div>
<div id="divs" buttons="#tabsButtons">
    <table id="table1" align="center" cellspacing="1" cellpadding="0">
    </table>
</div>


<style type="text/css">
    #table1 {
        border-left-color: #b9b9b9;
        border-top-color: #b9b9b9;
        border-left-width: 1px;
        border-top-width: 1px;
        border-left-style: dotted;
        border-top-style: dotted;
    }

        #table1 td {
            border-right-color: #b9b9b9;
            border-bottom-color: #b9b9b9;
            border-right-width: 1px;
            border-bottom-width: 1px;
            border-right-style: dotted;
            border-bottom-style: dotted;
        }

    .tdtitle {
        background-color: rgb(247, 247, 247);
        width: 80px;
        font-family: 'Microsoft YaHei';
        font-size: 12px;
        text-align: center;
    }

    .asLabel .mini-textbox-border,
    .asLabel .mini-textbox-input,
    .asLabel .mini-buttonedit-border,
    .asLabel .mini-buttonedit-input,
    .asLabel .mini-textboxlist-border {
        border: 0;
        background: none;
        cursor: default;
    }

    .asLabel .mini-buttonedit-button,
    .asLabel .mini-textboxlist-close {
        display: none;
    }

    .asLabel .mini-textboxlist-item {
        padding-right: 8px;
    }
</style>

<script type="text/javascript">
    var LayoutDef = @Html.Raw(ViewBag.LayoutDef)
    @Html.Raw(ViewBag.JS)
    @Html.Raw(ViewBag.EnumList)
</script>

