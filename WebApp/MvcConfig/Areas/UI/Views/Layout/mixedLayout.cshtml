@section scripts{
    <script src="@Url.Content("/MvcConfig/Scripts/layout.js")" type="text/javascript"></script>
    <script src="@Url.Content("/MvcConfig/Scripts/mixed.js")" type="text/javascript"></script>
}
<div style="height: 25px; width: 100%; background-color: seashell; line-height: 25px;" id="topBtn">
</div>
<div class="mini-splitter" style="width: 100%; height: 100%;" id="splitter">
    <div size="160" showcollapsebutton="true" id="ltree">
        <div id="txtSearch">
            <input id="key" class="mini-textbox" style="width: 100px; margin-left: 5px;" onenter="onKeyEnter" />
            <a class="mini-button" style="width: 40px;" onclick="search()">查询</a>
        </div>
        <div class="mini-fit" style="height: 100%;">
            <ul id="tree" class="mini-tree" style="width: 100%;">
            </ul>
        </div>
    </div>
    <div id="editForm" style="display: none;">
        <div id='tabs' class='mini-tabs' style='width: 100%; height: 100%;' activeindex='0'>
            <div id="treeList" title="树维护">
                <div id="tabsButtons">
                    <a class="mini-button" iconcls="icon-add" plain="true" onclick="addRow()" id="btnAdd">添加</a>
                    <a class="mini-button" iconcls="icon-remove" plain="true" onclick="delRow()" id="btnDel">移除</a>
                    <a class="mini-button" iconcls="icon-save" plain="true" onclick="saveList()" id="btnSave">保存</a>
                </div>

                <div class='mini-fit' id='gridTreeList' style='width: 100%; height: 100%;'></div>
            </div>
        </div>
    </div>

</div>


<script type="text/javascript">
    var LayoutDef = @Html.Raw(ViewBag.LayoutDef)
    @Html.Raw(ViewBag.JS)
    @Html.Raw(ViewBag.EnumList)
</script>

