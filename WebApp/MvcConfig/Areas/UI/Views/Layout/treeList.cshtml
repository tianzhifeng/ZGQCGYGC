@section scripts{
    <script src="@Url.Content("/MvcConfig/Scripts/layout.js")" type="text/javascript"></script>
    <script src="@Url.Content("/MvcConfig/Scripts/treeList.js")" type="text/javascript"></script>
}
<div class="mini-splitter" style="width: 100%; height: 100%;" id="divs">
    <div size="200" showcollapsebutton="true" id="ltree">
        <div id="txtSearch">
            <input id="keySearch" class="mini-textbox" style="width: 140px;margin-top:2px; margin-left:5px;" onenter="onKeyEnter" />
            <a class="mini-button" style="width: 40px;" plain="true" onclick="search()">查询</a>
        </div>
        <div class="mini-fit">
            <ul id="tree" class="mini-tree" style="width: 100%;">
            </ul>
        </div>
    </div>
    <div size="60%" showcollapsebutton="false">
        <div class="mini-toolbar gw-grid-toolbar" style="height: 22px; width: 100%">
            <div id="_btn" style="float: left; padding-left: 2px;"></div>
            <div id="sysBtn" style="float: right; height: 28px; width: 300px; text-align: right;">
                <a class="mini-button" iconcls="icon-download" onclick="ExportExcel()" id="download">导出Excel</a>
                <input id="key" class="mini-buttonedit gw-searchbox" style="width: 120px;" emptytext="请输入查询内容" onenter="keySearch" onclick="search()" />
                <a class="mini-button" id="_paraQuery" onclick="showWindow('queryWindow')" iconcls="icon-find" plain="true">参数查询</a>   
            </div>
        </div>
        <div class="mini-fit" id="divList" style="width: 100%; height: 30%;"></div>

    </div>

</div>
<div id="queryWindow" class="mini-window" title="参数" style="width: 300px; min-height: 100px;"
    showmodal="true" allowresize="false" allowdrag="true">
    <div class="queryDiv">
        <form id="queryForm" method="post">
        </form>
        <div>
            <a class="mini-button" onclick="filter()" iconcls="icon-find" style="margin-right: 20px;">
                确定</a> <a class="mini-button" onclick="clearQueryForm('queryForm')" iconcls="icon-undo">
                    清空</a>
        </div>
    </div>
</div>

<script type="text/javascript">
    var LayoutDef = @Html.Raw(ViewBag.LayoutDef)  
    @Html.Raw(ViewBag.JS)
    @Html.Raw(ViewBag.EnumList)
</script>
