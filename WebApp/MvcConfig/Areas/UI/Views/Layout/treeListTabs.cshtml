@section scripts{
    <script src="@Url.Content("/MvcConfig/Scripts/layout.js")" type="text/javascript"></script>
    <script src="@Url.Content("/MvcConfig/Scripts/treeListTab.js")" type="text/javascript"></script>
}
<div style="width: 100%; height: 100%;" id="divs" class="mini-splitter">
    <div size="200" showcollapsebutton="true" id="ltree">
        <div id="txtSearch">
            <input id="keySearch" class="mini-textbox" style="width: 140px;margin-top:2px; margin-left:5px;" onenter="onKeyEnter" />
            <a class="mini-button" style="width: 40px;" plain="true" onclick="search()">查询</a>
        </div>
        <div class="mini-tree" id="tree">
        </div>
    </div>
    <div showcollapsebutton="false" id="rdiv">
        <div style="height: 50%;">

            <div id="tabsButtons" style="height: 22px; width: 100%">
                <div id="_btn" style="float: left; padding-left: 2px;"></div>
                <div id="sysBtn" style="float: right; height: 28px; width: 300px; text-align: right;">
                    <a class="mini-button" iconcls="icon-download" onclick="ExportExcel()" id="download">导出Excel</a>
                    <input id="key" class="mini-buttonedit gw-searchbox" style="width: 120px;" emptytext="请输入查询内容" onenter="keySearch" onclick="search()" />
                    <a class="mini-button" id="_paraQuery" onclick="showWindow('queryWindow')" iconcls="icon-find" plain="true">参数查询</a>                  
                </div>
            </div>

            <div class="mini-fit" id="divList" style="width: 100%; height: 100%; overflow: auto;" buttons="#tabsButtons"></div>

        </div>
        <div style="height: 49%; margin-top: 5px;">
            <div id="tabsButtons2">
                <div id="_btn2" style="float: left; padding-left: 2px;"></div>
            </div>
            <div id="tabs" class="mini-tabs" activeindex="0" style="width: 100%; height: 100%;" buttons="#tabsButtons2">
            </div>
        </div>

    </div>
</div>
<div id="queryWindow" class="mini-window" title="参数" style="width: 300px; min-height: 100px;"
    showmodal="true" allowresize="false" allowdrag="true">
    <div class="queryDiv">
        <form id="queryForm" method="post">
        </form>
        <div>
            <a class="mini-button" onclick="filter()" iconcls="icon-find" style="margin-right: 20px;">
                确定</a> <a class="mini-button" onclick="clearQueryForm('queryForm')" iconcls="icon-undo">
                    清空</a>
        </div>
    </div>
</div>
<script type="text/javascript">
    var LayoutDef = @Html.Raw(ViewBag.LayoutDef)  
    @Html.Raw(ViewBag.JS)
    @Html.Raw(ViewBag.EnumList)
</script>
<style type="text/css">
    table[id^="table"] {
        border-left-color: #b9b9b9;
        border-top-color: #b9b9b9;
        border-left-width: 1px;
        border-top-width: 1px;
        border-left-style: dotted;
        border-top-style: dotted;
    }

     table[id^="table"] td {
        border-right-color: #b9b9b9;
        border-bottom-color: #b9b9b9;
        border-right-width: 1px;
        border-bottom-width: 1px;
        border-right-style: dotted;
        border-bottom-style: dotted;
   }

    .tdtitle {
        background-color: rgb(247, 247, 247);
        width: 80px;
        font-family: 'Microsoft YaHei';
        font-size: 12px;
        text-align: center;
    }
    .asLabel .mini-textbox-border,
    .asLabel .mini-textbox-input,
    .asLabel .mini-buttonedit-border,
    .asLabel .mini-buttonedit-input,
    .asLabel .mini-textboxlist-border
    {
        border:0;background:none;cursor:default;
    }
    .asLabel .mini-buttonedit-button,
    .asLabel .mini-textboxlist-close
    {
        display:none;
    }
    .asLabel .mini-textboxlist-item
    {
        padding-right:8px;
    }    

</style>