<script type='text/javascript'>

    function onLitigationTypeChange() {

        var type = mini.getbyName("TypesOfLitigation").getValue();

        var project = $("#projectInfo");
        var summonProject = $("#summonProjectInfo");

        if (type == "主动起诉") {
            project.show();
            summonProject.hide();
            setProjectTrue();
            setSummonProjectFalse();
        } else if (type == "被动应诉") {
            project.hide();
            summonProject.show();
            setProjectFalse();
            setSummonProjectTrue();
        } else {
            setProjectFalse();
            setSummonProjectFalse();
            project.hide();
            summonProject.hide();
        }

        flowLoadMenubar();
    }

    function setProjectFalse() {
        mini.getbyName("NameOfCase").setRequired(false);
        mini.getbyName("BriefIntroductionOfTheCase").setRequired(false);
        mini.getbyName("ReasonsForTransfer").setRequired(false);
        mini.getbyName("ExpectedTarget").setRequired(false);
        mini.getbyName("OurBreach").setRequired(false);
    }

    function setProjectTrue() {
        mini.getbyName("NameOfCase").setRequired(true);
        mini.getbyName("BriefIntroductionOfTheCase").setRequired(true);
        mini.getbyName("ReasonsForTransfer").setRequired(true);
        mini.getbyName("ExpectedTarget").setRequired(true);
        mini.getbyName("OurBreach").setRequired(true);
    }

    function setSummonProjectFalse() {
        mini.getbyName("OurSuedDepartment").setRequired(false);
        mini.getbyName("ReceiptName").setRequired(false);
        mini.getbyName("BriefIntroductionToTheCaseOfSubpoena").setRequired(false);
        mini.getbyName("DescriptionOfKeyIssues").setRequired(false);
        mini.getbyName("EvidentialDataRequirements").setRequired(false);
        mini.getbyName("ScanningCopyOfLegalDocuments").setRequired(false);
    }
    function setSummonProjectTrue() {
        mini.getbyName("OurSuedDepartment").setRequired(true);
        mini.getbyName("ReceiptName").setRequired(true);
        mini.getbyName("BriefIntroductionToTheCaseOfSubpoena").setRequired(true);
        mini.getbyName("DescriptionOfKeyIssues").setRequired(true);
        mini.getbyName("EvidentialDataRequirements").setRequired(true);
        mini.getbyName("ScanningCopyOfLegalDocuments").setRequired(true);
    }
</script>
