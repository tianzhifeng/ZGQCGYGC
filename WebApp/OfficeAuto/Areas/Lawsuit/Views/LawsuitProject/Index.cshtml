<script type='text/javascript'>

    function onLitigationTypeChange() {

        var type = mini.getbyName("LitigationType").getValue();

        var project = $("#projectInfo");
        var summonProject = $("#summonProjectInfo");

        if (type == "主动起诉") {
            project.show();
            summonProject.hide();
            setProjectTrue();
            setSummonProjectFalse();
        } else if (type == "被动应诉") {
            project.hide();
            summonProject.show();
            setProjectFalse();
            setSummonProjectTrue();
        } else {
            setProjectFalse();
            setSummonProjectFalse();
            project.hide();
            summonProject.hide();
        }

        flowLoadMenubar();
    }

    function setProjectFalse() {
        mini.getbyName("ProjectName").setRequired(false);
        mini.getbyName("ProjectTotalAmount").setRequired(false);
        mini.getbyName("AmountIsNoMistake").setRequired(false);
        mini.getbyName("ActualFinalPayDate").setRequired(false);
        mini.getbyName("PayAmountAndWay").setRequired(false);
        mini.getbyName("CurrentAmountOwed").setRequired(false);
        mini.getbyName("FirstDate").setRequired(false);
        mini.getbyName("FirstWay").setRequired(false);
        mini.getbyName("SecondDate").setRequired(false);
        mini.getbyName("SecondWay").setRequired(false);
        mini.getbyName("ThirdDate").setRequired(false);
        mini.getbyName("ThirdWay").setRequired(false);
        mini.getbyName("TransferReason").setRequired(false);
        mini.getbyName("ExpectTarget").setRequired(false);
        mini.getbyName("OurBreachOfContract").setRequired(false);
    }

    function setProjectTrue() {
        mini.getbyName("ProjectName").setRequired(true);
        mini.getbyName("ProjectTotalAmount").setRequired(true);
        mini.getbyName("AmountIsNoMistake").setRequired(true);
        mini.getbyName("ActualFinalPayDate").setRequired(true);
        mini.getbyName("PayAmountAndWay").setRequired(true);
        mini.getbyName("CurrentAmountOwed").setRequired(true);
        mini.getbyName("FirstDate").setRequired(true);
        mini.getbyName("FirstWay").setRequired(true);
        mini.getbyName("SecondDate").setRequired(true);
        mini.getbyName("SecondWay").setRequired(true);
        mini.getbyName("ThirdDate").setRequired(true);
        mini.getbyName("ThirdWay").setRequired(true);
        mini.getbyName("TransferReason").setRequired(true);
        mini.getbyName("ExpectTarget").setRequired(true);
        mini.getbyName("OurBreachOfContract").setRequired(true);
    }

    function setSummonProjectFalse() {
        mini.getbyName("OurRespondentDepartmentUnit").setRequired(false);
        mini.getbyName("ReceiptName").setRequired(false);
        mini.getbyName("CaseBrief").setRequired(false);
        mini.getbyName("DescriptionOfKeyIssues").setRequired(false);
        mini.getbyName("EvidenceInformationRequirements").setRequired(false);
        mini.getbyName("ScannedLegalDocuments").setRequired(false);
    }
    function setSummonProjectTrue() {
        mini.getbyName("OurRespondentDepartmentUnit").setRequired(true);
        mini.getbyName("ReceiptName").setRequired(true);
        mini.getbyName("CaseBrief").setRequired(true);
        mini.getbyName("DescriptionOfKeyIssues").setRequired(true);
        mini.getbyName("EvidenceInformationRequirements").setRequired(true);
        mini.getbyName("ScannedLegalDocuments").setRequired(true);
    }
</script>
