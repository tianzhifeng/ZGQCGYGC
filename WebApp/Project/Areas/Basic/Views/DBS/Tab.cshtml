@{
    ViewBag.Title = "Tab";
}
<div class="mini-fit" style="padding-top: 5px;">
    <div id="mainTabs" class="mini-tabs" activeindex="0" style="width: 100%; height: 100%;">
        <div title="项目资料归集" name="DBSMain" url="DBSMain?ProjectInfoID=@ViewBag.ProjectInfoID">
        </div>
        <div title="归档资料清单" url="/MvcConfig/UI/List/PageView?TmplCode=DBSArchiveList&ProjectInfoID=@ViewBag.ProjectInfoID">
        </div>
    </div>
</div>
<script type="text/javascript">
    function pageLoad() {
        var tabs = mini.get("mainTabs");
        tabs.activeTab("DBSMain");
    }

</script>
<style type="text/css">
    body
    {
        overflow: hidden;
    }
</style>
