@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutTab.cshtml";
}

<div class="mini-toolbar" id="btnDiv">
    <table>
        <tr>
            <td style="width: 100%;"></td>
            <td id="btnRight">
                <input class="mini-buttonedit searchbox" id="key" emptytext="请输入员工名称" style="width: 200px;"
                       onenter="cusTabSearch();" onbuttonclick="cusTabSearch();" />
            </td>
        </tr>
    </table>
</div>
<div class="mini-fit" id="gridarea" style="overflow: hidden;">
    <iframe id="myframe" frameborder="0" src="" name="myframe" style="width: 100%; height: 100%;
        overflow-x: hidden;" border="0" scrolling="yes"></iframe>
</div>
<script type="text/javascript">

    var groupInfoID = getQueryString("GroupInfoID");

    $(function () {
        cusTabSearch()
    });

    function cusTabSearch() {
        var tabData = GetTabsData();
        var tabCondition = "";
        if (tabData.length != 0)
            tabCondition = tabData[0].value;
        var keyValue = mini.get("key").getValue();
        document.getElementById("myframe").src = "List?GroupInfoID=" + groupInfoID + "&EmployeeName=" + keyValue + "&TabCondition=" + tabCondition;
    }

</script>
