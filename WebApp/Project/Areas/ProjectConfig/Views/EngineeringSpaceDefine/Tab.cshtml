@{
    ViewBag.Title = "Tab";
}

<div class="mini-fit" style="padding-top: 5px;">
    <div id="mainTab" class="mini-tabs" activeindex="0" style="width: 100%; height: 100%;">
        <div title="功能配置" url="../EngineeringSpaceRes/ResDefine">
        </div>
        <div title="功能授权" url="../EngineeringSpaceRes/Authorize">
        </div>
    </div>
</div>
<script type="text/javascript">
    $(".mini-tabs").find("div").each(function () {
        $(this).attr("url", $(this).attr("url") + "?SpaceID=" + getQueryString("ID"))
    });
</script>
