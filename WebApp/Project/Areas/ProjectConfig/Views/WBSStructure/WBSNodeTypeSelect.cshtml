@{
    ViewBag.Title = "WBSNodeTypeSelect";
}
<div class="mini-toolbar gw-grid-toolbar">
    <table>
        <tr>
            <td>
                <a class="mini-button" iconcls="icon-add" onclick="returnValue()">选择</a>
            </td>
        </tr>
    </table>
</div>
<div class="mini-fit" style="height: 100px;">
    <div id="dataGrid" showpager="false" url="GetWBSNodes" class="mini-datagrid" style="width: 100%; height: 100%;"
        allowresize="false" idfield="id" multiselect="true">
        <div property="columns">
            <div type="checkcolumn">
            </div>
            <div field="text" width="200" headeralign="center" allowsort="true" align="left">
                名称</div>
            <div field="value" width="150" headeralign="center" allowsort="true" align="left">
                编号</div>
        </div>
    </div>
</div>
<script type="text/javascript">
    function returnValue() {
        var dataGrid = mini.get("dataGrid");
        var result = dataGrid.getSelecteds();
        closeWindow(result);
    }
</script>
